{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./src/static/compiled"
  },
  "include": ["src/ts/**/*.ts"],
  "exclude": ["node_modules"]
}
